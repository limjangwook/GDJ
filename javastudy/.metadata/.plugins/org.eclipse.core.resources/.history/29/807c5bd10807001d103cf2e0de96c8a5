package ex02_casting;

public class EX01 {

	public static void main(String[] args) {
		
		// 자동 형 변환
		// promotion이라고 한다.
		// 자동을 다른 데이터타입으로 변환하는 것을 말하다.
		// 작은 크기으 데이터타입이 큰 크기의 데이터타입으로 변환될때 자동으로 진행된다.
		
		long money = 10000; // 4바이트에 저장된  10000이 8바이트로 promotion된다.
		System.out.println(money);
		
		int score = 100;
		double realScore = score;  // 4바이트 int가 8바이트 double로 promotion 된다.
		System.out.println(realScore);

	}

}
